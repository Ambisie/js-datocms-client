{
  parser: "babel-eslint",
  extends: [
    "airbnb-base",
    "prettier"
  ],
  env: {
    es6: true,
    mocha: true,
    browser: true
  },
  parserOptions: {
    sourceType: "module"
  },
  rules: {
    "arrow-body-style": 0,
    "no-console": 0,
    "no-restricted-syntax": 0,
    "no-await-in-loop": 0
  }
}
